{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": true,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "quotemark": [true, "double", "jsx-double"],
    "no-console": true,
    "one-line": false,
    "member-access": true,
    "interface-name": [true, "never-prefix"],
    "typedef": [true, "call-signature", "parameter", "member-variable-declaration"],
    "no-consecutive-blank-lines": false,
    "max-line-length": [true, {"limit": 120, "ignore-pattern": "^import |^export {(.*?)}"}],
    "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
    "curly": false,
    "whitespace": [true, "check-module", "check-branch", "check-operator", "check-typecast"]
  },
  "rulesDirectory": []
}
